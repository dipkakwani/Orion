<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<experiment>
    <name>orion_exp</name>
    <defaults/>
    <clients>
        <client id="Client_0" ip="127.0.0.1">
            <config>
                <os>linux</os>
                <client_jar_file><!-- JAR FILE ABSOLUTE PATH--></client_jar_file>
                <client_class_name>edu.msu.cse.orion.client.OrionClient</client_class_name>
                <workload><!-- PROPERTIES.TXT ABSOLUTE PATH --></workload>
                <username><!-- USERNAME --></username>
                <password><!-- PASSWORD --></password>
                <working_directory><!-- WORKING DIR ABSOLUTE PATH --></working_directory>
                <protocol_log_level>finest</protocol_log_level>
                <framework_log_level>finest</framework_log_level>
                <protocol_properties>
                    <property key="num_of_datacenters" value="1"/>
                    <property key="num_of_partitions" value="2"/>
                    <property key="dc_id" value="0"/>
                    <property key="heartbeat_interval" value="20"/>
                    <property key="dsv_comutation_interval" value="10"/>
                    <property key="max_latency" value="15"/>
                </protocol_properties>
            </config>
            <connect_to>
                <server id="0_0" ip="127.0.0.1" port="2000"/>
                <server id="0_1" ip="127.0.0.1" port="4000"/>
            </connect_to>
        </client>

        <client id="Client_1" ip="127.0.0.1">
            <config>
                <os>linux</os>
                <client_jar_file><!-- JAR FILE ABSOLUTE PATH--></client_jar_file>
